<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="model.RecordEntity">
        <table name="record" schema="Project" />
        <attributes>
            <id name="id">
                <column name="id"/>
            </id>
            <basic name="username">
                <column name="username"/>
            </basic>
            <basic name="content">
                <column name="content"/>
            </basic>
            <basic name="chatdate">
                <column name="chatdate"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
