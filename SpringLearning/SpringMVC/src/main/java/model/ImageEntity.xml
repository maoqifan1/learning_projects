<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="model.ImageEntity">
        <table name="image" schema="Project" />
        <attributes>
            <id name="imageid">
                <column name="imageid"/>
            </id>
            <basic name="imagename">
                <column name="imagename"/>
            </basic>
            <basic name="imagesource">
                <column name="imagesource"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
